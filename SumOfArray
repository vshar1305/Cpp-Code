#include<iostream>
using namespace std;

int main()
{
    cout<<"Enter the size of an array"<<endl;
    int size;
    cin>>size;
    cout<<"Enter the array"<<endl;
    int arr[100];
    int sum=0;
    for(int i=0;i<size;i++)
    {
        cin>>arr[i];
        sum=sum+arr[i];
    }
    cout<<"THe sum of an array is "<<sum<<endl;
}

#include<iostream>
#include<stack>
using namespace std;

int main()
{
    string str="Vishal";
    stack<char>st;

    for(int i=0;i<str.length();i++)
    {
        char ch=str[i];
        st.push(ch);
    }

    cout<<"The Reversed string is:-"<<endl;
    for(int i=0;i<str.length();i++)
    {
        char ans = st.top();
        st.pop();
        cout<<ans;
    }

   
}
